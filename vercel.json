{
  "buildCommand": "",
  "outputDirectory": "",
  "framework": null,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/socket.io/.*", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/public/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/server.js" }
  ]
}
